
@model IEnumerable<WebApplicationMVC.Models.Папка>
 
<h2>Список папок</h2>
<p><a asp-action="Create">Добавить папку</a></p>
<table class="table">
     <tr><th>Название</th><th>КодРодительскойПапки</th></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Название</td>
            <td>@item.КодРодительскойПапки</td>
        </tr>
    }
</table>