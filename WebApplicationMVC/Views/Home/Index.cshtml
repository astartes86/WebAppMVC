
@model IEnumerable<WebApplicationMVC.Models.Folder>
 
<h2>Добро пожаловать в тестовую программу Проводник</h2>
<p><a asp-action="Create">Добавить папку</a></p>
<table class="table">
     <tr><th>КодПапки</th><th>Название</th><th>КодРодительскойПапки</th></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.codefolder</td>
            <td>@item.name</td>
            <td>@item.codeparentfolder</td>
        </tr>
    }
</table>